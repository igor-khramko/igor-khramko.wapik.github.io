section.header.header-contact-page
  .container.header__content
    .header__left
        .header__company-name Wapik
        .header__phone-number
            span.header__phone-icon &#xf4b8;
            |+62 202 555 0117
    .header__right
        .header__navigation.navigation
        ul.navigation
            li.navigation__item
                a.link(href='index.html') Home
            li.navigation__item
                a.link(href='about-us.html') About us
            li.navigation__item
